{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "WeatherApi": {
      "BaseUrl": "https://openweathermap.org/api",
      "Endpoint": "weather",
      "ApiKey": "WEATHER_API_KEY"
    },
    "NewsApi": {
      "BaseUrl": "https://newsapi.org/",
      "Endpoint": "everything",
      "ApiKey": "NEWS_API_KEY"
    },
    "SpotifyApi": {
      "BaseUrl": "https://developer.spotify.com/documentation/web-api/",
      "Endpoint": "search",
      "ApiKey": "SPOTIFY_API_KEY"
    }
  }
}
